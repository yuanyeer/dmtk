/*cell  */
.set-cell {
  height: 58px;
  /* background-color: #abc; */
  align-items: center;
  display: flex;
  border-bottom: 1px solid #F0F0F0;
  background-color: #fff;
}
.set-cell image {
  width: 28px;
  height: 28px;
  border-radius: 50%;
  margin-left: 15px;
  margin-right: 10px;
}
.set-cell label {
  font-size: 13px;
}
.set-cell .arrow {
  position: absolute;
  right: 8px;
  width: 20px;
  height: 20px;
}
.cell-content .set-cell:last-child {
  border-bottom: 0px;
}
.set-cell .subTitle {
  position: absolute;
  right: 43px;
  color: lightgray;
}