/* pages/courses/courses.wxss */
@import '../../css/global-css/base.wxss';

.top {
  background-color: #F8F8F8;
  width: 100%;
  height: 90rpx;
  position:fixed;
  top: 0;
  left: 0;
}

.top-item{
  height: 100%;
  width: 33.333%;
  font-size: 28rpx;
  color: #333333;
  justify-content: center;
  align-items: center;
}
.top-item image {
  margin-left: 10rpx;
}
.selected {
  color: #69BE79;
}