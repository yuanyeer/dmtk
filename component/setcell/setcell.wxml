<view class='cell-content'>
  <view class='set-cell' wx:for="{{dataArr}}" wx:key="id" bindtap='_celltapEvent' data-link="{{item.link}}" style="height:{{ui_setting.height}}px" data-idx="{{item.idx}}">
      <image src='{{item.icon}}'></image>
      <label>{{item.title}}</label>
      <label class='subTitle'>{{item.subTitle}}</label>
      <image class='arrow' src='../../images/my/arrow.png'></image>
      <button class="kefu" wx:if="{{item.idx == '101'}}" open-type="contact">客服</button>
  </view>
</view>